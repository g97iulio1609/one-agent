{
  "id": "copilot",
  "version": "2.0.0",
  "type": "agent",
  "description": "OneCoach AI Copilot - orchestrates domain-specific agents for chat",
  "interface": {
    "input": { "$ref": "copilot:input" },
    "output": { "$ref": "copilot:output" }
  },
  "mcpServers": {},
  "config": {
    "tier": "balanced",
    "temperature": 1,
    "maxSteps": 20,
    "maxTokens": 8192,
    "timeout": 300000,
    "executionMode": "stream",
    "skipSynthesis": false
  }
}
