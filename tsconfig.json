{
  "extends": "../../packages/tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022"],
    "moduleResolution": "bundler",
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "noUncheckedIndexedAccess": false,
    "baseUrl": "../..",
    "rootDir": "./src",
    "paths": {
      "@onecoach/one-agent/*": ["./submodules/one-agent/src/*"]
    },
    "outDir": "./dist"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
