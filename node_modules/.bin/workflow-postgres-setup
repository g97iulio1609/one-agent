#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules/@workflow/world-postgres/bin/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules/@workflow/world-postgres/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules/@workflow/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules/@workflow/world-postgres/bin/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules/@workflow/world-postgres/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules/@workflow/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/@workflow+world-postgres@4.1.0-beta.27_@electric-sql+pglite@0.3.15_@opentelemetry+api@1_fc6d445155a23b3e3ad679568f3f44c5/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@workflow/world-postgres/bin/setup.js" "$@"
else
  exec node  "$basedir/../@workflow/world-postgres/bin/setup.js" "$@"
fi
